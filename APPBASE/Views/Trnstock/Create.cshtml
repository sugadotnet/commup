@{
    ViewBag.Title = "Mutasi " + ViewBag.Storagebasename + " Barang / Furniture";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}

@using (Html.BeginForm())
{

<div class="box box-info">
    <div class="box-header">
        <h3 class="box-title"></h3>
    </div><!-- /.box-header -->
    <div class="box-body">
        @{Html.RenderPartial("~/Views/Trnstock/_Inputs.cshtml");}
        <hr />
        @{Html.RenderPartial("~/Views/Trnstock/_Inputs_items.cshtml");}
    </div><!-- /.box-body -->
    <div class="box-footer">
        <div class="row">
            <div class="col-xs-12">
                <button type="submit" class="btn btn-primary btn-flat">Simpan Data</button>
                @Html.ActionLink("Back to List", "Index")
            </div>
        </div><!-- /.row -->
    </div><!-- /.box-footer -->
</div><!-- /.box -->
    
}

<!-- Modal -->
@{Html.RenderPartial("_PartialDialogLookup");}


@section scripts {

