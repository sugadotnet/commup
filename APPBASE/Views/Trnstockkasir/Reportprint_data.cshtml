@model APPBASE.Models.TrnstockVM
@using APPBASE.Helpers
@using APPBASE.Svcutil
@using APPBASE.Svcbiz
@{int nNO = 0;}

<!-- title Table -->
<table width="700pt">
<!-- title row -->
    <tr class="title">
        <td colspan="4" style="text-align:left;">Mutasi Barang/Furniture dari @ViewBag.Storagebasename</td>
        <td colspan="3" style="text-align:right;">No: @Model.TRN_CODE</td>
    </tr>
    <tr class="title">
        <td colspan="7">&nbsp;</td>
    </tr>
    <tr class="title">
        <td colspan="2">Tanggal : @hlpConvertionAndFormating.ConvertDateToStringMMMFmt(Model.TRN_DT)</td>
    </tr>
    <tr class="title">
        <td colspan="2">Alamat : @Model.TRN_DESC</td>
    </tr>
</table>

